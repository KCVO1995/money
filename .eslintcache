;\n      this.record.note = note;\n      // eslint-disable-next-line @typescript-eslint/camelcase\n      this.record.is_expend = is_expend;\n      // eslint-disable-next-line @typescript-eslint/camelcase\n      this.record.tag_id = tag_id;\n      // eslint-disable-next-line @typescript-eslint/camelcase\n      this.record.created_at = created_at;\n      // eslint-disable-next-line @typescript-eslint/camelcase\n      this.record.updated_at = updated_at;\n      // eslint-disable-next-line @typescript-eslint/camelcase\n      this.record.user_id = user_id;\n    }, e => {\n      console.log('e', e);\n    });\n  }\n\n  saveRecord() {\n    if (this.record) {\n      if (!this.record.tag_id) {\n        return alert('选择一个标签，分类记录有助于统计');\n      } else if (this.record.id > 0) { // TODO 编辑\n        store.dispatch('updateRecord', this.record);\n        this.$router.back();\n      } else {\n        store.dispatch('createRecord', this.record);\n        this.$router.back();\n      }\n      this.record.note = '';\n      // eslint-disable-next-line @typescript-eslint/camelcase\n      this.record.tag_id = undefined;\n    }\n\n  }\n}\n\n</script>\n\n<style lang=\"scss\" scoped>\n.global {\n  display: flex;\n  flex-direction: column;\n  width: 100vw;\n  height: 100vh;\n}\n\n.main {\n  margin: auto;\n}\n\n\n</style>\n<style lang='scss' scoped>\n</style>\n","/Users/leejacky/code/study/money/src/views/NotFound.vue",[],"/Users/leejacky/code/study/money/src/views/Register.vue",[],"/Users/leejacky/code/study/money/src/views/Statistics.vue",[],"/Users/leejacky/code/study/money/src/views/User.vue",[],"/Users/leejacky/code/study/money/tests/unit/example.spec.ts",[],"/Users/leejacky/code/study/money/vue.config.js",[],{"ruleId":"229","severity":1,"message":"230","line":27,"column":23,"nodeType":"231","messageId":"232","endLine":27,"endColumn":26,"suggestions":"233"},{"ruleId":"229","severity":1,"message":"230","line":35,"column":27,"nodeType":"231","messageId":"232","endLine":35,"endColumn":30,"sugg